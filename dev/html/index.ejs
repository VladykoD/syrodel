<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="UTF-8">
    <title>Сыродел</title>
    <%= htmlWebpackPlugin.tags.headTags.filter((tag) => (tag.tagName === 'meta'))
            .join('')
    %>
    <meta name="viewport" content="user-scalable=no, width=device-width, height=device-height, initial-scale=1">
    <meta name="theme-color" content="#f6a917">
    <meta name="api" content="<%= process.env.API_URL %>">

    <meta property="og:locale" content="ru_RU">
    <meta property="og:title" content="Cheese">
    <meta property="og:url" content="<%= process.env.PUBLIC_PATH %>">
    <meta property="og:type" content="website">
    <meta property="og:image" content="<%= process.env.PUBLIC_PATH %>/static/soc.png">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="630">
    <link rel="image_src" href="<%= process.env.PUBLIC_PATH %>/static/soc.png">
    <!--      <meta property="og:description" content="-&#45;&#45;">-->
    <meta property="og:site_name" content="Cheese">


<!--      <link rel="icon" href="/ico/32.png" type="image/x-icon">-->
<!--      <link rel="shortcut icon" href="/ico/32.png" type="image/x-icon">-->
<!--      <link rel="apple-touch-icon" href="/ico/1024.png">-->
<!--      <link rel="apple-touch-icon" sizes="76x76" href="/ico/76.png">-->
<!--      <link rel="apple-touch-icon" sizes="120x120" href="/ico/120.png">-->
<!--      <link rel="apple-touch-icon" sizes="152x152" href="/ico/152.png">-->
<!--      <link rel="apple-touch-icon" sizes="180x180" href="/ico/180.png">-->

    <%= htmlWebpackPlugin.tags.headTags.filter((tag) => (tag.tagName === 'link'))
            .join('')
    %>
</head>
<body>
<noscript>You need to enable JavaScript to run this app.</noscript>
<div id="root"></div>
<%= htmlWebpackPlugin.tags.bodyTags.filter((tag) => (
        tag.tagName !== 'script' ||
        tag.attributes.src.includes('main')
))
%>
<%= htmlWebpackPlugin.tags.headTags.filter((tag) => (tag.tagName === 'script'))
        .join('')
%>
</body>
</html>
